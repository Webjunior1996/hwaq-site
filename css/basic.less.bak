/* flex预设 */
.flex {
    display: flex;
}
.flex-i {
    display: inline-flex;
}
/*分成几列*/
.flex-column(@i, @max) when (@i <= @max) {
    .flex-@{i}>* {
        width: 100/@i * 1%;
    }
    .flex-column(@i + 1, @max);
}
.flex-column(1, 10);
.flex-s {
    flex-shrink: 0;
}
.flex-dr {
    flex-direction: row;
}
.flex-dc {
    flex-direction: column;
}
.flex-drr {
    flex-direction: row-reverse;
}
.flex-as {
    align-items: flex-start;
}
.flex-ac {
    align-items: center;
}
.flex-ae {
    align-items: flex-end;
}
.flex-cc {
    align-content: center;
}
.flex-ca {
    align-content: space-around;
}
.flex-cb {
    align-content: space-between;
}
.flex-jc {
    justify-content: center;
}
.flex-ja {
    justify-content: space-around;
}
.flex-jb {
    justify-content: space-between;
}
.flex-js {
    justify-content: flex-start;
}
.flex-je {
    justify-content: flex-end;
}
.flex-w {
    flex-wrap: wrap;
}
.flex-wn {
    flex-wrap: nowrap;
}

.gap(@i, @max,@gapRatio) when (@i <= @max) {
    @scaledValue: ceil(@i * @gapRatio);
    .flexGap-@{i} {
        gap: @scaledValue * 1px;
    }
    .radius-@{i} {
        border-radius: @scaledValue * 1px;
    }
    .gap(@i + 1, @max,@gapRatio);
}
.gap(1, 80,1);

/* grid预设*/
.grid {
    display: grid;
}
/*分成几列*/
.gridColumns(@i, @max,@gapRatio) when (@i <= @max) {
    @scaledValue: ceil(@i * pow(0.21, (1-@gapRatio)));
    .grid-@{i} {
        grid-template-columns: repeat(@scaledValue,1fr);
    }
    .gridColumns(@i + 1, @max,@gapRatio);
}
.gridColumns(1, 12,1);
.grid-span {
    grid-column: span 2;
}
@media screen and (max-width: 1450px){
    .gridColumns(1, 12,.85);

}
@media screen and (max-width: 1250px){
    .gridColumns(1, 12,.8);

}
@media screen and (max-width: 1000px){
    .gridColumns(1, 12,.6);
    .flex-wap {
        flex-wrap: wrap;
        &>* {
            width: 100% !important;
            padding: 0 !important;
        }
    }

}
@media screen and (max-width: 700px){
    .gridColumns(1, 12,.4);
    .grid-span {
        grid-column: span 1;
    }

}
@media screen and (max-width: 500px){
    .gridColumns(1, 12,.1);

}

/* 边距预设 */
//比例计算 (1-@ratio)*margin // 向上取整
.generate(@i, @max,@marginRatio) when (@i <= @max) {
    //@scaledValue: ceil(@i * @marginRatio);
    @scaledValue: ceil(@i * pow(0.35, (1-@marginRatio)));
    .mt-@{i} {
        margin-top: @scaledValue * 1px;
    }
    .mb-@{i} {
        margin-bottom: @scaledValue * 1px;
    }
    .ml-@{i},html[lang='sa'] .mr-@{i} {
        margin-left: @scaledValue * 1px;
    }
    .mr-@{i},html[lang='sa'] .ml-@{i} {
        margin-right: @scaledValue * 1px;
    }
    .pt-@{i} {
        padding-top: @scaledValue * 1px;
    }
    .pb-@{i} {
        padding-bottom: @scaledValue * 1px;
    }
    .pl-@{i},html[lang='sa'] .pr-@{i} {
        padding-left: @scaledValue * 1px;
    }
    .pr-@{i},html[lang='sa'] .pl-@{i} {
        padding-right: @scaledValue * 1px;
    }
    .slickGap-@{i} {
        margin: 0 -@scaledValue/2*1px;
        li {
            padding: 0 @scaledValue/2*1px;
        }
    }
    .generate(@i + 1, @max,@marginRatio);
}
.generate(0, 200,1);

/*字体预设*/
// 比例计算 (1-@@fontRatio)*font // 向上取整
// 处理常规情况 特殊情况 比如 font-size:200px;  由main.css定义
.fontSize(@i, @max, @scale) when (@i <= @max) {
    @scaledValue: max(ceil(@i * pow(0.3, (1-@scale))),14);
    .font-@{i} {
        font-size: @scaledValue*1px;
    }
    .fontSize(@i + 1, @max, @scale);
}
.fontSize(14, 100,1);

.fontEm(@i, @max, @scale) when (@i <= @max) {
    @scaledValue: ceil(@i * pow(0.5, (1-@scale)));
    .font-em-@{i} {
        font-size: @scaledValue*.1em;
    }
    .fontEm(@i + 1, @max, @scale);
}
.fontEm(0, 100,1);
/* 宽度定义 */
.width(@i, @max) when (@i <= @max) {
    .wid-@{i} {
        width: @i * 1%;
    }
    .width(@i + 1, @max);
}
.width(0, 100);

/* 响应式上的处理 */
