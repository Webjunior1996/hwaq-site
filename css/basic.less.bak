        position:absolute;top:0;left:0;width:100%;height:100%;
        transform:translateX(100%);
        transition:.28s;
        display:flex;flex-direction:column;
    }
    .panel.active{
        transform:translateX(0);
    }

    /* panel header */
    .panel-header{
        display:flex;align-items:center;padding:16px;border-bottom:1px solid #eee;
    }
    .back-btn{background:none;border:none;color:var(--color);font-size:18px;cursor:pointer;margin-right:8px;}
    .close-btn{background:none;border:none;color:var(--color);font-size:20px;cursor:pointer;margin-left:8px;}
    .panel-title{font-weight:700;font-size:18px;flex:1;color:#333;}

    /* panel body */
    .panel-body{
        padding:20px;
        flex:1;
        display:flex;flex-direction:column;
        gap:12px;
    }
    .menu-menu-container ul {list-style:none;padding:0;margin:0;display:flex;flex-direction:column;gap:12px;}
    .menu-menu-container ul ul {
        display: none;
    }
    .menu-item{
        padding:20px;border-radius:16px;background:#e7e7e7;color:black;font-size:18px;text-align:center;cursor:pointer;position:relative;
    }
    .menu-item:hover,.menu-item.active{background:var(--color);color: white}
    .chev{position:absolute;right:16px;top:50%;transform:translateY(-50%);}

    /* footer */
    .lang-panel{display:flex;gap:8px;padding:12px;border-top:1px solid #eee;justify-content:center;font-size: 16px;}
    .lang-switch{background:var(--color);color:#fff;padding:6px 12px;border-radius:12px;text-decoration:none;cursor:pointer;}
    .menu-footer{display:flex;gap:8px;padding:12px;border-top:1px solid #eee;justify-content:center;}
    .share-btn{background:var(--color);color:#fff;font-size:16px;width:2.5em;height:2.5em;border-radius:100%;border:none;cursor:pointer;}
}
