/*侧边按钮*/
#sidebar {
    font-size: 20px;
    position: fixed;
    right: 0;
    bottom: 27%;
    z-index: 20;
    -webkit-perspective: 500px;
    perspective: 500px;
    transform-style: preserve-3d;
    ul {
        li {
            width: 2.5em;
            height: 2.5em;
            display: flex;
            background-color: var(--color);
            align-items: center;
            justify-content: center;
            color: white;
            border-top: 1px solid white;
            cursor: pointer;
            cursor: hand;
            position: relative;
            -webkit-transition: .5s;
            -moz-transition: .5s;
            -ms-transition: .5s;
            -o-transition: .5s;
            transition: .5s;
            z-index: 0;
            box-shadow: 0 0 .5em rgba(0,0,0,.1);
            &:first-child {
                border-top-left-radius: .2em;
                border-top: none;
            }
            &:last-child {
                border-bottom-left-radius: .2em;
            }
            &.top {
                background-color: white;
            }
            .child {
                position: absolute;
                right: 0;
                top: 0;
                background-color: white;
                padding: .5em;
                border-radius: .5em;
                margin-right: 1em;
                -moz-box-shadow: 0 0 .5em rgba(0,0,0,.1);
                -o-box-shadow: 0 0 .5em rgba(0,0,0,.1);
                box-shadow: 0 0 .5em rgba(0,0,0,.1);
                transform: rotate3d(0,1,0,90deg);
                transform-origin: top right;
                -webkit-transition: .5s;
                -moz-transition: .5s;
                -ms-transition: .5s;
                -o-transition: .5s;
                transition: .5s;
                z-index: -1;
                opacity: 0;
                color: black;
                * {
                    font-size: .8em;
                    min-height: 100%;
                }
                a {
                    color: var(--color);
                }
                img {
                    max-width: none;
                }
                &:before {
                    content: '';
                    border-left: .5em solid white;
                    border-top: .3em solid transparent;
                    border-bottom: .3em solid transparent;
                    position: absolute;
                    left: 100%;
                    top: 1em;
                }
            }
            #ewm2,#ewm {
                width: 120px;
                img {
                    max-width: 100%;
                }
            }
            &:hover {
                background-color: black;
                color: white;
                z-index: 1;
                .child {
                    right: 100%;
                    opacity: 1;
                    transform: rotate3d(0,1,0,0deg);
                }
            }
        }
    }
}

/*小部件*/
.play-btn {
    font-size: 20px;
    width: 3em;
    height: 3em;
    border-radius: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    color: white;
    background-color: var(--color);
    position: absolute;
    top: 50%;
    left: 50%;
    -webkit-transform: translate(-50%,-50%);
    -moz-transform: translate(-50%,-50%);
    -ms-transform: translate(-50%,-50%);
    -o-transform: translate(-50%,-50%);
    transform: translate(-50%,-50%);
    z-index: 1;
    cursor: pointer;
    cursor: hand;
    i {
        border-left: .6em solid white;
        border-top: .5em solid transparent;
        border-bottom: .5em solid transparent;
    }
    &:before,&:after {
        content: '';
        width: 100%;
        height: 100%;
        position: absolute;
        top: 0;
        left: 0;
        background-color: inherit;
        border-radius: inherit;
        z-index: -1;
        -webkit-animation:ks 2s linear infinite;
        -o-animation:ks 2s linear infinite;
        -ms-animation:ks 2s linear infinite;
        -moz-animation:ks 2s linear infinite;
        animation:ks 2s linear infinite;
    }
    &:before {
        -webkit-animation-delay: .6s;
        -o-animation-delay: .6s;
        -ms-animation-delay: .6s;
        -moz-animation-delay: .6s;
        animation-delay: .6s;
    }
}

.slickBtn {
    font-size: 20px;
    display: flex;
    direction: ltr;
    div {
        width: 2em;
        height: 2em;
        border-radius: 100%;
        display: flex;
        align-items: center;
        justify-content: center;
        background-color: var(--color);
        color: white;
        cursor: pointer;
        cursor: hand;
        -webkit-transition: .5s;
        -moz-transition: .5s;
        -ms-transition: .5s;
        -o-transition: .5s;
        transition: .5s;
        &:hover {
            background-color: var(--color);
            color:white;
            border-color: var(--color);
        }
        &.next {
            margin-left: 1em;
        }
    }
    &.pos {
        position: static;
        div {
            position: absolute;
            top: 50%;
            -webkit-transform: translateY(-50%);
            -moz-transform: translateY(-50%);
            -ms-transform: translateY(-50%);
            -o-transform: translateY(-50%);
            transform: translateY(-50%);
            z-index: 1;
            &.prev {
                right: 102%;
            }
            &.next {
                left: 102%;
                margin-left: 0;
            }
        }
    }
}

.slickDots {
    font-size: 10px;
    ul {
        display: flex;
        align-items: center;
        justify-content: center;
        li {
            width: 1em;
            height: 1em;
            border-radius: 100%;
            border: 1px solid var(--color);
            background-color: var(--color);
            -webkit-transition: .5s;
            -moz-transition: .5s;
            -ms-transition: .5s;
            -o-transition: .5s;
            transition: .5s;
            cursor: pointer;
            cursor: hand;
            margin: 0 .5em;
            button {
                display:none;
            }
            &.slick-active {
                background-color: transparent;
            }
        }
    }
}
#wap-bottom {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    z-index: 20;
    background-color: var(--color);
    color: white;
    height: 56px;
    display: none;
    &>div {
        height: inherit;
    }
    i {
        font-size: 1.5em;
        line-height: 1;
    }
    .left,.right {
        width: 100%;
        a {
            width: 100%;
            text-transform: capitalize;
            border-left: 1px solid rgba(255,255,255,.2);
            &:first-child {
                border-left: none;
            }
        }
    }
    .center {
        width: 60px;
        flex-shrink: 0;
        height: 100%;
        margin: 0 5px;
        i {
            font-size: 2em;
        }
        a {
            height: 120%;
            background-color: var(--color);
            -moz-box-shadow: 0 0 5px rgba(0,0,0,.1);
            -o-box-shadow: 0 0 5px rgba(0,0,0,.1);
            box-shadow: 0 0 5px rgba(0,0,0,.1);
            margin-top: -20%;
            border-top-left-radius: 5px;
            border-top-right-radius: 5px;
        }
    }
}
.yuy {
    position: relative;
    height: 100%;
    span {
        height: 100%;
    }
    ul {
        font-size: 14px;
        width: 120px;
        background-color: white;
        color: black;
        display: none;
        position: absolute;
        left: 0;
        top: 100%;
        li {
            border-top: 1px solid #eee;
            a {
