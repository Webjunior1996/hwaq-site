        &~#innerMobileBanner,&~#indexMobileBanner,&~#banner {
            margin-top: 0 !important;
        }
    }

}
@media (max-width: 1000px) {
    /* 手机端内页banner */
    #innerMobileBanner {
        display: block;
        padding: 40px 0;
        width: 100%;
        background: #f5f5f5;
        border-radius: 10px;
        box-shadow: 0 4px 10px rgba(0, 0, 0, 0.1);
    }
    #innerMobileBanner .container .title {
        font-size: 20px;
        line-height: 1.6;
        color: #333;
    }
    #innerMobileBanner .container .mbx {
        font-size: 14px;
        color: #666;
        gap: 5px;
        line-height: 2;
    }
    #innerMobileBanner .container .mbx span {
        color: #999;
    }
    #innerMobileBanner .container .mbx a {
        color: #999;
        transition: all 0.5s;
        text-decoration: underline;
        text-decoration-color: rgba(0, 0, 0, 0);
    }
    #innerMobileBanner .container .mbx a:hover {
        color: var(--color);
        text-decoration-color: var(--color);
    }

    /* 手机端内页banner 背景换为网站主题色的版本*/
    #innerMobileBanner.colorActive {
        background-color: var(--color);
    }
    #innerMobileBanner.colorActive .container .title {
        color: #fff;
    }
    #innerMobileBanner.colorActive .container .mbx {
        color: #fff;
    }
    #innerMobileBanner.colorActive .container .mbx span,
    #innerMobileBanner.colorActive .container .mbx a {
        color: #fff;
    }
    #innerMobileBanner.colorActive .container .mbx a:hover {
        text-decoration-color: #fff;
    }
}
@media (max-width: 500px) {
    /* 手机端首页banner */
    #indexMobileBanner {
        margin-top: 40px;
        display: block;
    }
    #indexMobileBanner ul li {
        height: calc(80vh - 40px);
    }
    #indexMobileBanner ul li .box {
        background-color: var(--color);
        height: 100%;
        background-size: cover;
    }
    #indexMobileBanner ul li .box .title {
        font-size: 24px;
    }
    #indexMobileBanner ul li .box .des {
        font-size: 16px;
    }
    /* 手机端首页banner 字体颜色改为白色版本 */
    #indexMobileBanner.colorWhite {
        color: #fff;
    }
}

