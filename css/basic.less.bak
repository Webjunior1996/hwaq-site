/*******图片移上放大效果*******/
.img-box {
    overflow: hidden;
    display: block;
    position: relative;
}
.img-box img {
    transition: 1s ease;
    width: 100%;
}
.img-box:hover img {
    transform: scale(1.1, 1.1);
}
@keyframes ks {
    from {
        -webkit-transform: scale(1);
        -moz-transform: scale(1);
        -ms-transform: scale(1);
        transform: scale(1);
        opacity: 0;
    }
    10% {
        -webkit-transform: scale(1);
        -moz-transform: scale(1);
        -ms-transform: scale(1);
        transform: scale(1);
        opacity: 1;
    }
    80% {
        -webkit-transform: scale(2);
        -moz-transform: scale(2);
        -ms-transform: scale(2);
        transform: scale(2);
        opacity: .2;
    }
    to {
        -webkit-transform: scale(2);
        -moz-transform: scale(2);
        -ms-transform: scale(2);
        transform: scale(2);
        opacity: 0;
    }
}
