#Advertising {
    position: fixed;
    font-size: 10px;
    width: 60em;
    padding: 3em 2em;
    box-shadow: 0 0 1em rgba(0,0,0,.1);
    max-width: 90%;
    z-index: 22;
    background-color: white;
    .close {
        font-size: 1.5em;
        width: 2em;
        height: 2em;
        border-radius: 100%;
        position: absolute;
        right: 0;
        top: 0;
        transform: translate(50%,-50%);
        background-color: var(--color);
        color: white;
        cursor: pointer;
        cursor: hand;
        &:hover {
            background-color: black;
        }
    }
    .content {
        max-height: 70vh;
        overflow: auto;
    }
    &.lt {
        left: 0;
        top: 2em;
        transform: unset;
    }
    &.lc {
        left: 0;
        top: 50%;
        transform: translate(0,-50%);
    }
    &.lb {
        left: 0;
        top: auto;
        bottom: 0;
        transform: unset;
    }
    &.rt {
        left: auto;
        right: 0;
        top: 2em;
        transform: unset;
    }
    &.rc {
        left: auto;
        right: 0;
        top: 50%;
        transform: translate(0,-50%);
    }
    &.rb {
        left: auto;
        right: 0;
        top: auto;
        bottom: 0;
        transform: unset;
    }
}
